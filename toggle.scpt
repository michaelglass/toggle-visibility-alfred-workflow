set appPath to "/System/Volumes/Data/Applications/Atom.app"set target to POSIX file appPath as aliastell application "System Events"		set ps to (every process whose background only is false)		repeat with i in ps		set p to POSIX path of file of i		set p to POSIX file p as alias		if p is target then			if frontmost of i then				set visible of i to false			else				set frontmost of i to true			end if			return		end if	end repeatend telltell application appPath to activate